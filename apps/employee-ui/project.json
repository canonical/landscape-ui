{
  "name": "employee-ui",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/employee-ui/src",
  "projectType": "application",
  "tags": [],
  "// targets": "to see all targets run: nx show project employee-ui --web",
  "targets": {
    "serve": {
      "executor": "@nx/vite:dev-server",
      "options": {
        "buildTarget": "employee-ui:build",
        "configFile": "apps/employee-ui/vite.config.ts"
      },
      "configurations": {
        "production": {
          "buildTarget": "employee-ui:build:production"
        }
      }
    },
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "configFile": "apps/employee-ui/vite.config.ts",
        "outputPath": "dist/apps/employee-ui"
      },
      "configurations": {
        "production": {
          "fileReplacements": []
        }
      }
    }
  }
}
