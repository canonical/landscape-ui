{
  "name": "landscape-webapp-new",
  "private": true,
  "version": "1.23.0",
  "type": "module",
  "engines": {
    "node": ">=22.15.1"
  },
  "scripts": {
    "dev": "vite --host",
    "build": "pnpm run lint:ci && tsc && vite build",
    "build:e2e": "pnpm run lint:ci && tsc && vite build --mode e2e",
    "lint:ci": "eslint --fix . --config eslint.ci.config.mjs --ext .ts,.tsx",
    "preview": "vite preview",
    "test": "pnpm exec playwright test",
    "test:saas": "VITE_SELF_HOSTED_ENV=false pnpm exec playwright test --project=saas",
    "test:self-hosted": "VITE_SELF_HOSTED_ENV=true pnpm exec playwright test --project=self-hosted",
    "test:ui": "pnpm exec playwright test --ui",
    "vitest": "vitest",
    "coverage": "vitest run --coverage",
    "test:codegen": "pnpm exec playwright codegen",
    "test:report": "pnpm exec playwright show-report",
    "tcm:run": "tcm \"src/**\" --pattern *.module.scss",
    "tcm:watch": "pnpm tcm:run --watch"
  },
  "dependencies": {
    "@canonical/react-components": "3.8.2",
    "@monaco-editor/react": "4.7.0",
    "@sentry/react": "^10.37.0",
    "@tanstack/react-query": "5.90.20",
    "axios": "1.13.5",
    "buffer": "6.0.3",
    "chart.js": "4.5.1",
    "classnames": "2.5.1",
    "downshift": "9.0.13",
    "formik": "2.4.9",
    "moment": "2.30.1",
    "path-to-regexp": "8.3.0",
    "react": "19.2.4",
    "react-chartjs-2": "5.3.1",
    "react-dom": "19.2.4",
    "react-router": "7.13.0",
    "usehooks-ts": "3.1.1",
    "vanilla-framework": "4.40.0",
    "yup": "1.7.1"
  },
  "overrides": {
    "@types/react": "19.1.12"
  },
  "devDependencies": {
    "@eslint/js": "9.39.2",
    "@playwright/test": "1.58.0",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/exec": "7.1.0",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "12.0.2",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.2",
    "@testing-library/user-event": "14.6.1",
    "@types/node": "22.19.1",
    "@types/react": "19.2.10",
    "@types/react-dom": "19.2.3",
    "@types/react-table": "7.7.20",
    "@vitejs/plugin-react": "5.1.2",
    "@vitest/coverage-v8": "4.0.18",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "7.0.1",
    "globals": "17.2.0",
    "jsdom": "27.4.0",
    "msw": "2.4.11",
    "prettier": "3.8.1",
    "sass": "1.97.3",
    "sass-embedded": "1.97.3",
    "semantic-release": "25.0.2",
    "stylelint": "^16.26.1",
    "stylelint-config-standard-scss": "^16.0.0",
    "stylelint-order": "7.0.1",
    "typed-css-modules": "0.9.1",
    "typescript": "5.9.3",
    "typescript-eslint": "8.54.0",
    "vite": "7.3.1",
    "vite-plugin-eslint": "1.8.1",
    "vitest": "4.0.18"
  }
}
