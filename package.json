{
  "name": "landscape-webapp-new",
  "private": true,
  "version": "1.11.1",
  "type": "module",
  "engines": {
    "node": ">=20.11.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "VITE_API_URL=/api/v2/ VITE_API_URL_OLD=/api/ VITE_ROOT_PATH=/new_dashboard/ VITE_SELF_HOSTED_ENV=true VITE_REPORT_VIEW_ENABLED=false tsc && vite build",
    "preview": "vite preview",
    "prepare": "husky",
    "test": "playwright test",
    "test:ui": "VITE_SELF_HOSTED_ENV=true playwright test --ui",
    "vitest": "vitest",
    "coverage": "vitest run --coverage",
    "test:codegen": "playwright codegen localhost:5173",
    "test:report": "playwright show-report"
  },
  "dependencies": {
    "@canonical/react-components": "1.9.0",
    "@monaco-editor/react": "4.6.0",
    "@tanstack/react-query": "5.64.2",
    "axios": "1.7.9",
    "buffer": "6.0.3",
    "chart.js": "4.4.7",
    "classnames": "2.5.1",
    "downshift": "9.0.8",
    "formik": "2.4.6",
    "moment": "2.30.1",
    "path-to-regexp": "8.2.0",
    "react": "18.3.1",
    "react-chartjs-2": "5.3.0",
    "react-dom": "18.3.1",
    "react-router-dom": "6.26.2",
    "usehooks-ts": "3.1.0",
    "vanilla-framework": "4.20.2",
    "yup": "1.6.1"
  },
  "devDependencies": {
    "@eslint/js": "9.19.0",
    "@playwright/test": "1.50.0",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "11.0.1",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.2.0",
    "@testing-library/user-event": "14.6.1",
    "@types/chart.js": "2.9.41",
    "@types/eslint__js": "8.42.3",
    "@types/react": "18.3.18",
    "@types/react-dom": "18.3.5",
    "@types/react-table": "7.7.20",
    "@types/react-test-renderer": "18.3.1",
    "@vitejs/plugin-react": "4.3.4",
    "@vitest/coverage-v8": "3.0.4",
    "eslint": "9.19.0",
    "eslint-config-prettier": "10.0.1",
    "eslint-plugin-prettier": "5.2.3",
    "eslint-plugin-react": "7.37.4",
    "globals": "15.14.0",
    "husky": "9.1.7",
    "jsdom": "26.0.0",
    "lint-staged": "15.4.2",
    "msw": "2.4.9",
    "prettier": "3.4.2",
    "sass": "1.83.4",
    "semantic-release": "24.2.1",
    "stylelint": "16.13.2",
    "stylelint-config-standard-scss": "14.0.0",
    "stylelint-order": "6.0.4",
    "stylelint-prettier": "5.0.2",
    "typescript": "5.7.3",
    "typescript-eslint": "8.21.0",
    "vite": "6.0.11",
    "vite-plugin-eslint": "1.8.1",
    "vitest": "3.0.4"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,md,html}": [
      "eslint --cache",
      "prettier --check"
    ],
    "src/**/*.scss": "stylelint"
  }
}
