#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/runner/work/landscape-ui/landscape-ui/node_modules/.pnpm/vitest@4.0.18_@types+node@22.19.1_jsdom@28.0.0_msw@2.4.11_typescript@5.9.3__sass-embedded@1.97.3_sass@1.97.3_yaml@2.8.1/node_modules/vitest/node_modules:/home/runner/work/landscape-ui/landscape-ui/node_modules/.pnpm/vitest@4.0.18_@types+node@22.19.1_jsdom@28.0.0_msw@2.4.11_typescript@5.9.3__sass-embedded@1.97.3_sass@1.97.3_yaml@2.8.1/node_modules:/home/runner/work/landscape-ui/landscape-ui/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/runner/work/landscape-ui/landscape-ui/node_modules/.pnpm/vitest@4.0.18_@types+node@22.19.1_jsdom@28.0.0_msw@2.4.11_typescript@5.9.3__sass-embedded@1.97.3_sass@1.97.3_yaml@2.8.1/node_modules/vitest/node_modules:/home/runner/work/landscape-ui/landscape-ui/node_modules/.pnpm/vitest@4.0.18_@types+node@22.19.1_jsdom@28.0.0_msw@2.4.11_typescript@5.9.3__sass-embedded@1.97.3_sass@1.97.3_yaml@2.8.1/node_modules:/home/runner/work/landscape-ui/landscape-ui/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../vitest@4.0.18_@types+node@22.19.1_jsdom@28.0.0_msw@2.4.11_typescript@5.9.3__sass-embedded@1.97.3_sass@1.97.3_yaml@2.8.1/node_modules/vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../../../../../../vitest@4.0.18_@types+node@22.19.1_jsdom@28.0.0_msw@2.4.11_typescript@5.9.3__sass-embedded@1.97.3_sass@1.97.3_yaml@2.8.1/node_modules/vitest/vitest.mjs" "$@"
fi
